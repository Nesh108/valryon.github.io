@model Portfolio.ViewModels.BlogViewModel
@{
    Layout = "~/Views/Shared/_Layout.cshtml";

    Model.Title = "Welcome traveler";
}

<div class="row-fluid">
    <h2>Welcome again</h2>
    <p>

    </p>
</div>

<div>
    @if (Model.Articles != null)
    {
        foreach (Portfolio.Models.Article article in Model.Articles)
        {
        <a href="@article.Url" >
            <p>@article.Title</p>
        </a>
        }
    }
</div>