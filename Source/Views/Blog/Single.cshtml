@model Portfolio.ViewModels.SingleViewModel

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    Model.Title = Model.Content.Article.Title;

    Model.Metas["og:url"] = Model.Content.Article.Url;
    Model.Metas["og:title"] = Model.Title;
    Model.Metas["og:description"] = Model.Content.Article.Description;
}


@Html.Partial("ArticleView", Model.Content);

